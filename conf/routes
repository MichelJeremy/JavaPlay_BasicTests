# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Display Home page
GET     /                            controllers.Application.index

# do things
GET     /formHello                  controllers.Application.hello
POST    /formHello                  controllers.Application.loginSubmit
GET     /graph                      controllers.Application.viewGraph
GET     /graphv2                    controllers.Application.viewGraphv2
GET     /d3Graph                    controllers.Application.d3Graph
GET		/d3Graphv2					controllers.Application.d3Graphv2
GET		/d3Graphv2Java				controllers.Application.d3Graphv2Java
GET		/testpage					controllers.Application.bootstrapSandstone

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
